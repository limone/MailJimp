<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss} [%t] %p [%c] %m%n</pattern>
    </encoder>
  </appender>
  <logger name="mailjimp" additivity="false" level="DEBUG">
    <appender-ref ref="stdout"/>
  </logger>
  <logger name="org.apache.cxf.interceptor" additivity="false" level="DEBUG">
    <appender-ref ref="stdout"/>
  </logger>
  <root level="WARN">
    <appender-ref ref="stdout"/>
  </root>
</configuration>